{
  "rewrites": [
    {
      "source": "/stats/js/script.js",
      "destination": "https://plausible.io/js/script.outbound-links.js"
    },
    {
      "source": "/stats/api/event",
      "destination": "https://plausible.io/api/event"
    }
  ],
  "crons": [
    {
      "path": "/api/syncChannels",
      "schedule": "27 * * * *"
    },
    {
      "path": "/api/syncNewVideos",
      "schedule": "0/5 * * * *"
    },
    {
      "path": "/api/syncVideos",
      "schedule": "1/5 * * * *"
    },
    {
      "path": "/api/matchTags",
      "schedule": "4/10 * * * *"
    }
  ]
}